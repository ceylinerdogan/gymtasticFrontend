{
  "appId": "com.gymtastic.app",
  "appName": "GymTastic",
  "webDir": "dist",
  "bundledWebRuntime": false,
  "server": {
    "allowNavigation": ["*"],
    "cleartext": true
  },
  "android": {
    "allowMixedContent": true
  },
  "plugins": {
    "Camera": {
      "permissions": ["camera"]
    },
    "CapacitorHttp": {
      "enabled": true
    }
  }
}
